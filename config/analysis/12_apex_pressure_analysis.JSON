        {"type": "heatmap", "data_key": "pred_pop", "cmap": "Blues", "title": "Final Predator (SmallFish) Population"},
        {"type": "heatmap", "data_key": "prey_pop", "cmap": "Greens", "title": "Final Prey (Zooplankton) Population"}
